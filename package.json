{
  "name": "@jabra/twilio-flex-call-control-plugin",
  "version": "1.0.0-beta.1",
  "main": "lib/index.js",
  "scripts": {
    "start": "craco start",
    "build": "npm run build:dist && npm run build:lib",
    "build:dist": "craco build && ncp build/index.js dist/plugin-jabra-call-control.js",
    "build:lib": "cross-env REACT_APP_BUILD_LIB=true craco build && ncp build/index.js lib/index.js && ncp build/index.js.map lib/index.js.map",
    "bootstrap": "flex-check-start",
    "postinstall": "npm run bootstrap",
    "prestart": "npm run bootstrap"
  },
  "dependencies": {
    "@craco/craco": "^5.5.0",
    "@twilio/flex-ui": "^1.13.0",
    "babel-polyfill": "^6.26.0",
    "craco-config-flex-plugin": "^2.9.1",
    "flex-plugin": "^2.9.1",
    "jabra-browser-integration": "git+https://github.com/gnaudio/jabra-browser-integration.git#v3.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-redux": "^7.1.1",
    "react-scripts": "^3.1.2",
    "react-tooltip": "^3.11.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@twilio/flex-ui": "^1",
    "cross-env": "^6.0.0",
    "ncp": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
